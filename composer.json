{
  "name": "yuncms/yii2-payment",
  "description": "The Yii Payment module",
  "keywords": [
    "yii",
    "yii2-payment",
    "payment"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yuncms/yii2-user": "~2.0",
    "endroid/qrcode": "~1.0",
    "whichbrowser/parser": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\payment\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "payment",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "basePath": "@yuncms/payment/message"
      },
      "migrationNamespace": "yuncms/payment/migrations",
      "backend": {
        "class": "yuncms\\payment\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\payment\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
